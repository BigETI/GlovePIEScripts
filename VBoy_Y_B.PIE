Keyboard.Right = (Wiimote1.Classic.Right or (MapRange(Wiimote1.Classic.Joy1X, -1,1, -1,1) > 0.75))
Keyboard.F = (MapRange(Wiimote1.Classic.Joy2Y, -1,1, -1,1) < (-0.75))
Keyboard.V = (MapRange(Wiimote1.Classic.Joy2Y, -1,1, -1,1) > 0.75)
Keyboard.C = (MapRange(Wiimote1.Classic.Joy2X, -1,1, -1,1) < (-0.75))
Keyboard.B = (MapRange(Wiimote1.Classic.Joy2X, -1,1, -1,1) > 0.75)
